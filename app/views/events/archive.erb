<h1>Минувшие события</h1>
<table>
	<% for event in @events%>
  <tr>
    <td><h2><%= event.actual_time.strftime('%d-%m-%Y  %H:%M') %></h2></td>
    <td><h2 class="concert_name"><%= event.name %></h2></td>
  </tr>
  <tr>
    <td><%= link_to image_tag(event.photo.url(:medium)), event_path(event) %></td>
    <td class="desc"><%= (event.description).split("\n")[0..2] %></td>
  </tr>

<% end %>
</table>